{
    "name": "python-sphinx-highlight",
    "displayName": "Python Sphinx Highlighter",
    "description": "Highlight Sphinx-specific markup inside Python docstrings.",
    "version": "0.3.0",
    "publisher": "leonhard-s",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/injection.json",
                "scopeName": "python-sphinx.injection",
                "injectTo": [
                    "source.python"
                ]
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/leonhard-s/python-sphinx-highlighter"
    }
}